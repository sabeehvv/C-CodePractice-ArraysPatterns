#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int i,j,k=1;
	for(i=1;i<10;i++){
		for(j=k;j-k<=i;j++){
			printf("%d ",j);
		}k=j+1;
		printf("d");
	}
	return EXIT_SUCCESS;
}
