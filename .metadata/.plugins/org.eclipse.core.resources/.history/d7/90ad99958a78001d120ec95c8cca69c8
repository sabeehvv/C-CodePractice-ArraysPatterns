#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int i,j=0;
	for(i=1;i<5;i++){
		for(j=1;j<=i;j++){
			printf("%d ",i);
		}printf("\n");
	}
	return EXIT_SUCCESS;
}
